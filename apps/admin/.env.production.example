# Database
DATABASE_URL="prisma+postgres://accelerate.prisma-data.net/?api_key=<YOUR_PRISMA_ACCELERATE_API_KEY>"

# Public URLs
WEB_PUBLIC_URL="https://<YOUR_DOMAIN>"
ADMIN_PUBLIC_URL="https://admin.<YOUR_DOMAIN>"
PUBLIC_DOMAIN="<YOUR_DOMAIN>"

# Authentication
AUTH_SECRET="<RANDOM_SECRET_STRING>" # Generate with: openssl rand -base64 32
AUTH_TRUST_HOST="True"

# Revalidation
REVALIDATION_SECRET="<RANDOM_REVALIDATION_SECRET>"

# Other settings
DEBUG_MODE="false"
LOG_LEVEL="info"